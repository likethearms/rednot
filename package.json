{
  "name": "rednot",
  "version": "0.1.20",
  "description": "Redux notification system",
  "main": "dist/main.js",
  "scripts": {
    "bundle": "webpack",
    "lint": "eslint . --ext .js,.tsx,.ts",
    "test": "jest src",
    "start": "webpack-dev-server --config development.config.js --mode development --open --hot",
    "build": "rm -rf dist && tsc --project tsconfig.typing.json && webpack --config build.config.js --mode production",
    "prepublish": "npm run build",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags"
  },
  "keywords": [
    "redux",
    "notifications",
    "react"
  ],
  "author": "Henrik Salmela <henrik.salmela@gmail.com>",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "jest": {
    "setupFilesAfterEnv": ["<rootDir>/setupTests.ts"],
    "collectCoverageFrom": [
      "src/**/*",
      "!src/demo/**/*"
    ]
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-transition-group": "^4.2.2"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-transition-group": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.2",
    "@types/react-transition-group": "^4.2.2",
    "@types/redux-mock-store": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "install": "^0.13.0",
    "jest": "^24.9.0",
    "npm": "^6.11.2",
    "prop-types": "^15.7.2",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "style-loader": "^1.0.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0",
    "webpack-node-externals": "^1.7.2"
  }
}
